# vim: ft=sh

ok file /etc/apt/sources.list.d/tor-client.list tor-client.list
if did_update; then
	gpg --keyserver keys.gnupg.net --recv A3C4F0F979CAA22CDBA8F512EE8CBC9E886DDD89
	gpg --export A3C4F0F979CAA22CDBA8F512EE8CBC9E886DDD89 | sudo apt-key add -
	apt update
fi

ok apt tor 
ok apt deb.torproject.org-keyring
groups debian-tor | grep root || usermod -a -G root debian-tor
